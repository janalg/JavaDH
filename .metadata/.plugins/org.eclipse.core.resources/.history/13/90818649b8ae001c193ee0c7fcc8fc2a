package jogoPOO;

public class ArmaDois implements Armas {
	public static final int qtdMunicao = 10;
	public static final float tempoRecarregar = 5.7f;
	@Override
	public void atirar() {
		System.out.println("Você atirou! Quantidade de munição atual: " + qtdMunicao);
	}

	@Override
	public void recarregar() {
		System.out.println("Recarregando " + tempoRecarregar + "s");
	}
}
